&project
  project_rootname = 'miniMesh'
/
&raw_grid
  grid_format = 'aflr3'
  data_format = 'default'
  swap_yz_axes = .false.
/
&governing_equations
  eqn_type = 'compressible'
  viscous_terms = 'laminar'
/
&reference_physical_properties
  mach_number     = 0.3
  angle_of_attack = 5.0
  reynolds_number = 2.91137e4
  temperature     = 300.0
  temperature_units = 'Kelvin'
/
&inviscid_flux_method
  flux_construction       = 'roe'
  flux_limiter            = 'none'
  smooth_limiter_coeff    = 1.0
  freeze_limiter_iteration = 100
/
&code_run_control
  restart_write_freq = 10
  restart_read       = 'off'
  steps              =  10
/
&nonlinear_solver_parameters
  time_accuracy = "2ndorderOPT"
  time_step_nondim =   1.0
  subiterations =          60
  schedule_iteration(1:2) = 1, 10
  schedule_cfl(1:2) = 1.0, 400.0
  schedule_cflturb(1:2) = 5.0,50.0
/
&gust_data
  ngust_omc = 1
  l_gust_omc(1) = 1.0
  tref_gust_omc(1) = 8.0
  u_gust_omc(1) = 0.0
  v_gust_omc(1) = 0.0
  w_gust_omc(1) = -0.01
/
&elasticity_gmres
  tol = 1.e-20
  tol_abs = 1.e-30
/
&global
  moving_grid = .true.
  boundary_animation_freq = 1
  volume_animation_freq = 1
/
&slice_data
  nslices	= 1
  slice_z(:)	= .true.
  slice_location(:) = 2.5
/
&volume_output_variables
  export_to = 'tecplot'
  temperature = .true.
  x = .true.
  y = .true.
  z = .true.
/
&boundary_output_variables
  number_of_boundaries = 1
  boundary_list = '2'
  temperature = .true.
  x = .true.
  y = .true.
  z = .true.
/
&massoud_output
  funtofem_include_skin_friction = .true.
/
